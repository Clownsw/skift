{
    "id": "efi-x86_64",
    "type": "target",
    "props": {
        "toolchain": "clang",
        "arch": "x86",
        "sub": "64",
        "vendor": "unknown",
        "sys": "efi",
        "abi": "ms",
        "encoding": "utf16",
        "freestanding": true
    },
    "tools": {
        "cc": {
            "cmd": "clang",
            "args": [
                "-target",
                "x86_64-unknown-windows",
                "-ffreestanding",
                "-fno-stack-protector",
                "-fshort-wchar",
                "-mno-red-zone"
            ]
        },
        "cxx": {
            "cmd": "clang++",
            "args": [
                "-target",
                "x86_64-unknown-windows",
                "-ffreestanding",
                "-fno-stack-protector",
                "-fshort-wchar",
                "-mno-red-zone"
            ]
        },
        "ld": {
            "cmd": "clang++",
            "args": [
                "-target",
                "x86_64-unknown-windows",
                "-nostdlib",
                "-Wl,-entry:efi_main",
                "-Wl,-subsystem:efi_application",
                "-fuse-ld=lld-link"
            ]
        },
        "ar": {
            "cmd": "llvm-ar",
            "args": [
                "rcs"
            ]
        },
        "as": {
            "cmd": "nasm",
            "args": [
                "-f",
                "win64"
            ]
        }
    }
}
